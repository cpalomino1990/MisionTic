
@page
@model ProyectoCiclo3.App.Frontend.Pages.CreateAeropuertoModel
@{
    var aeropuerto = Model.Aeropuerto;
}
<link rel="stylesheet" href="~/css/breadcrumb.css"/>
 <script src="~/js/breadcrumb.js" asp-append-version="true"></script>
  <div id="orangeUI">
        <div class="breadcrumbs">
        <ul>
            <li><a href="/Index">Home</a><div class="bread-shadow">Home</div></li>
            <li><a href="/Aeropuerto/List">Listas</a><div class="bread-shadow">Listas</div></li>
            <li><a href="/Aeropuerto/Edit"class="current">Editar</a><div class="bread-shadow">Editar</div></li>
          </ul>
    </div> 
    
 </div>

<div class="text-center">
  <form method="POST">
    <label for="id" class="form-label">Id:</label>
    <input type="text" class="form-control" id="id" name="id" placeholder="Id" asp-for="@aeropuerto.id">
    <label for="nombre" class="form-label">nambre:</label>
    <input type="text" class="form-control" id="nombre" name="nombre" placeholder="Nombre" asp-for="@aeropuerto.nombre">
    <label for="ciudad" class="form-label">Ciudad:</label>
    <input type="number" class="form-control" id="ciudad" name="ciudad" placeholder="Ciudad" asp-for="@aeropuerto.ciudad">
    <label for="pais" class="form-label">Pais: </label>
    <input type="number" class="form-control" id="pais" name="pais" placeholder="Pais" asp-for="@aeropuerto.pais">
    <label for="coord_x" class="form-label">Coordenada X:</label>
    <input type="number" class="form-control" id="coord_x" name="coord_x" placeholder="Coord x" asp-for="@aeropuerto.coord_x">
    <label for="coord_y" class="form-label">Coordenada y:</label>
    <input type="number" class="form-control" id="coord_y" name="coord_y" placeholder="Coord y" asp-for="@aeropuerto.coord_y">
    <hr>
    <button type="submit" class="btn btn-success">Enviar</button>
  </form>
</div>


